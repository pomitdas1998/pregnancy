{
    "version":2,
    "builds":[
        {
            "scr":"pregnancy_project/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxlambdaSize":"15mb","runtime":"python3.11.3"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-buid",
            "config":{
                "distDir":"staticfiles_build"

            }
        }
    ],

    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"poll/wsgi.py"
        }
    ]
}